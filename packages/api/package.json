{
  "name": "@MagnunAVFAzion/faststore-api",
  "version": "1.12.35",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/api.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "browserslist": "node 12",
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MagnunAVFAzion/faststore.git",
    "directory": "packages/api"
  },
  "scripts": {
    "develop": "tsdx watch --entry ./local/server.ts --target node --onSuccess \"node ./dist\"",
    "develop:no-server": "concurrently \"yarn generate -w\" \"tsdx watch\"",
    "build": "graphql-codegen --config codegen.yml && tsdx build",
    "lint": "eslint src/**/*.ts",
    "test": "tsdx test",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.2.0",
    "@rollup/plugin-graphql": "^1.0.0",
    "dataloader": "npm:@MagnunAVFAzion/dataloader@^2.1.0",
    "fast-deep-equal": "^3.1.3",
    "graphql": "npm:@MagnunAVFAzion/graphql@^15.8.0",
    "isomorphic-unfetch": "^3.1.0",
    "p-limit": "^3.1.0"
  },
  "devDependencies": {
    "@faststore/shared": "^1.12.35",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "concurrently": "^6.2.1",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "^15.6.0",
    "jest-transform-graphql": "^2.1.0",
    "ts-jest": "25.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "resolutions": {
    "**/dataloader": "npm:@MagnunAVFAzion/dataloader@^2.1.0",
    "**/graphql": "npm:@MagnunAVFAzion/graphql@^15.8.0"
  }
}
